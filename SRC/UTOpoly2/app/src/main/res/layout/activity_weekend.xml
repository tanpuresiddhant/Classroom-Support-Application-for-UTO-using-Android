<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_weekend"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.utopoly.WeekendActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:text="1.Please enter your name:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView40" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:hint="Name"
                android:ems="10"
                android:id="@+id/name" />

            <TextView
                android:text="2.Please enter classroom number:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView39" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:hint="Classroom number"
                android:ems="10"
                android:id="@+id/classroomnum" />

            <TextView
                android:text="3.Check clocks, lights, ceiling and wall damage:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView38" />

            <RadioGroup
                android:id="@+id/radioGroup1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton24"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton2"
                    android:layout_weight="1" />

            </RadioGroup>

            <TextView
                android:text="4.whiteborads and trays cleaned:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView37" />

            <RadioGroup
                android:id="@+id/radioGroup2"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Cleaned"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton25"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not cleaned"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton13"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="5.Replace white rags and check yello rags:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView36" />

            <RadioGroup
                android:id="@+id/radioGroup3"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Replaced"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not replaced"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton26"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="6.Replace cleaning solution when needed:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView35" />

            <RadioGroup
                android:id="@+id/radioGroup4"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Replaced"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton28"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not replaced"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton27"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="7.Clean lectern:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView34" />

            <RadioGroup
                android:id="@+id/radioGroup5"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Cleaned"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton30"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not cleaned"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton29"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="8.Test Markers (replace if needed):"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView33" />

            <RadioGroup
                android:id="@+id/radioGroup6"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Replaced"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton32"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not replaced"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton31"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="9.Floor and student desk are cleaned and rearranged:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView32" />

            <RadioGroup
                android:id="@+id/radioGroup7"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Completed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton34"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not completed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton33"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="10.Furniture is accounted for:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView31" />

            <RadioGroup
                android:id="@+id/radioGroup8"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Checked"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton50"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not checked"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton49"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="11.Log into Mac and Windows OS:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView30" />

            <RadioGroup
                android:id="@+id/radioGroup9"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton52"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton51"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="12.Projector image, doc, cam and audio check"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView29" />

            <RadioGroup
                android:id="@+id/radioGroup10"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Checked"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton54"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not checked"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton53"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="13.Test laptop VGA and /or HDMI, audio, Ethernet:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView28" />

            <RadioGroup
                android:id="@+id/radioGroup11"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton56"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton55"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="14.Make sure all the computers and devices are still secured:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView42" />

            <RadioGroup
                android:id="@+id/radioGroup12"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Secured"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton58"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not secured"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton57"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="15.Log into the windows and Mac os at student stations"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView27" />

            <RadioGroup
                android:id="@+id/radioGroup13"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton60"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton59"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="16.Notate any descrepancies with LMRD at Windows login:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView41" />

            <RadioGroup
                android:id="@+id/radioGroup14"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Correct"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton62"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not correct"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton61"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="17.Clean the computers, screens, keyboards and mice:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView26" />

            <RadioGroup
                android:id="@+id/radioGroup15"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Cleaned"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton64"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not cleaned"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton63"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="18.Cable Management is ok:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView25" />

            <RadioGroup
                android:id="@+id/radioGroup16"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton66"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton65"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="19.Projector and TV are off:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView24" />

            <RadioGroup
                android:id="@+id/radioGroup17"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="On"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton68"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Off"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton67"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="20.Current classroom schedule:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView23" />

            <RadioGroup
                android:id="@+id/radioGroup18"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Present"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton70"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not present"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton69"
                    android:layout_weight="1" />
            </RadioGroup>

            <TextView
                android:text="21.Door(s) close by themselves"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView" />

            <RadioGroup
                android:id="@+id/radioGroup19"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioButton
                    android:text="Working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton72"
                    android:layout_weight="1" />

                <RadioButton
                    android:text="Not working"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/radioButton71"
                    android:layout_weight="1" />
            </RadioGroup>

            <Button
                android:text="Continue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/submit"
                android:layout_weight="1" />

        </LinearLayout>
    </ScrollView>
</RelativeLayout>
